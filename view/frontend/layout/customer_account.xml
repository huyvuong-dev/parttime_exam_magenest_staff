<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
<body>
    <referenceBlock name="customer_account_navigation">
        <block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-navigation-custom-link" after="-" >
            <arguments>
                <argument name="label" xsi:type="string" translate="true">Be a Staff</argument>
                <argument name="path" xsi:type="string">manage/staff</argument>
                <argument name="sortOrder" xsi:type="number">150</argument>
            </arguments>
        </block>
    </referenceBlock>
</body>
</page>